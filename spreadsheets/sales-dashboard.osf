  B1 = "Revenue";
  C1 = "Target";
  D1 = "Variance";
  E1 = "New Customers";
  F1 = "Churn %";
  
  A2 = "July";
  B2 = 5100000;
  C2 = 5000000;
  D2 = =B2-C2;
  E2 = 52;
  F2 = 3.5;
  
  A3 = "August";
  B3 = 5400000;
  C3 = 5000000;
  D3 = =B3-C3;
  E3 = 58;
  F3 = 3.2;
  
  A4 = "September";
  B4 = 5750000;
  C4 = 5000000;
  D4 = =B4-C4;
  E4 = 57;
